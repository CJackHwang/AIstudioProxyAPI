{
    "include": [
        "api_utils",
        "tests"
    ],
    "exclude": [
        "**/__pycache__",
        "**/*.pyc",
        ".git",
        ".venv",
        "node_modules",
        "deprecated_javascript_version",
        "errors_py",
        "logs",
        "build",
        "dist",
        "stream",
        "browser_utils"
    ],
    "pythonVersion": "3.12",
    "typeCheckingMode": "off",
    "useLibraryCodeForTypes": true,
    "reportMissingImports": "none",
    "reportMissingTypeStubs": "none",
    "reportGeneralTypeIssues": "none"
}
